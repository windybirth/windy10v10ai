		<snippet name="Pet">
			<Panel class="NewPatreonPet">
				<Panel id="PetImagePanel">
					<Image id="PetImage" />
				</Panel>
				<Label id="PetName" text="{s:PetName}" />
				<Label id="PetTier" text="{s:PetTier}" />
