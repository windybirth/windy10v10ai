					<Label id="EnablePetLabel" text="EQUIP" />
				</Button>
				<Panel id="RoshanOptions" />
			</Panel>
		</snippet>
		<snippet name="BabyRoshanOption">
			<Panel>
				<Button id="ChooseRoshan" />
			</Panel>
		</snippet>
		<snippet name="Emblem">
